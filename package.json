{
  "name": "lift-mend-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint --dir .",
    "lint-fix": "npm run lint -- --fix",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "check-all": "npm run lint && npm run lint-fix && npm run type-check && npm run format && npm run format-check",
    "codegen-server": "graphql-codegen --config graphql/codegen/server-codegen.yaml",
    "codegen-client": "graphql-codegen --config graphql/codegen/client-codegen.yml",
    "test": "jest --watchAll",
    "test:once": "jest --ci --reporters=default --reporters=jest-summary-reporter",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage --no-watch",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test:ci",
    "prisma-generate": "npx prisma generate",
    "prisma-migrate": "npx prisma migrate dev --name init",
    "prisma-push": "npx prisma db push",
    "prisma-pull": "npx prisma db pull",
    "prisma-redeploy": "npx prisma migrate deploy",
    "postinstall": "npx prisma generate"
  },
