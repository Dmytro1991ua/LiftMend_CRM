model JobTypes {
  id        Int      @id @default(autoincrement())
  job_types String[]
}

model Priorities {
  id         Int      @id @default(autoincrement())
  priorities String[]
}

model ElevatorTypes {
  id             Int      @id @default(autoincrement())
  elevator_types String[]
}

model BuildingNames {
  id             Int      @id @default(autoincrement())
  building_names String[]
}

model ElevatorLocations {
  id        Int      @id @default(autoincrement())
  locations String[]
}

model TechnicianNames {
  id    Int      @id @default(autoincrement())
  names String[]
}

model TechnicianSkills {
  id     Int      @id @default(autoincrement())
  skills String[]
}

model RepairJob {
  id                 String         @id @default(uuid())
  jobType            String
  jobDetails         String
  jobPriority        String
  elevatorType       String
  buildingName       String
  elevatorLocation   String
  technicianName     String
  technicianSkills   String[]
  contactInformation String
  startDate          DateTime
  endDate            DateTime
  calendarEventId    String?        @unique
  calendarEvent      CalendarEvent? @relation
}
