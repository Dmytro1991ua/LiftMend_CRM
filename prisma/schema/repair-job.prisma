model RepairJob {
  id               String            @id @default(uuid())
  jobType          String
  jobDetails       String
  elevatorType     String
  buildingName     String
  elevatorLocation String
  technicianName   String
  startDate        DateTime
  endDate          DateTime
  calendarEventId  String?           @unique
  jobPriority      String
  status           String            @default("Scheduled")
  actualEndDate    DateTime?         @db.Timestamptz(6)
  isOverdue        Boolean?
  createdAt        DateTime?         @default(now()) @db.Timestamp(6)
  updatedAt        DateTime?         @default(now()) @db.Timestamp(6)
  elevatorId       String?
  technicianId     String?
  calendarEvent    CalendarEvent?
  elevatorRecord   ElevatorRecord?   @relation(fields: [elevatorId], references: [id])
  technicianRecord TechnicianRecord? @relation(fields: [technicianId], references: [id])
}
